{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
        "username": {
        "type": "string",
        "minLength": 1,
        "maxLength": 30
        },
        "friendUsername": {
            "type": "string",
            "minLength": 1,
            "maxLength": 30
        },
        "since": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "username",
        "friendUsername"
    ]
}
  